{
  "name": "work-on-time",
  "version": "0.0.2-alpha.9",
  "description": "Simple job scheduler for Node.js with persistence store.",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "tap test/*test.js",
    "watch:dash": "parcel watch --dist-dir dist-dev --public-url ./ --no-autoinstall --log-level verbose dashboard/**/*.html",
    "build:dash": "parcel build --dist-dir dist --public-url ./ --no-autoinstall --log-level verbose dashboard/**/*.html",
    "watch:dev-server": "nodemon -i dashboard/views-src -i dist-dev -i dist dev-server.js",
    "release:pre": "npm version prerelease --preid=alpha"
  },
  "targets": {
    "main": false
  },
  "includePaths": [
    "node_modules"
  ],
  "author": "Gagan Bansal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gagan-bansal/work-on-time"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "bluebird": "^3.7.2",
    "cron": "^1.8.2",
    "extend": "^3.0.2",
    "mongodb": "^4.2.2",
    "p-queue": "^6.6.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "clone": "^2.1.2",
    "connect-livereload": "^0.6.1",
    "datatables.net": "^1.12.1",
    "datatables.net-dt": "^1.12.1",
    "dayjs": "^1.11.4",
    "delay": "^5.0.0",
    "express": "^4.18.1",
    "jquery": "^3.6.0",
    "livereload": "^0.9.3",
    "mockdate": "^3.0.5",
    "mongodb-memory-server": "^8.3.0",
    "parcel": "^2.6.2",
    "process": "^0.11.10",
    "sinon": "^13.0.1",
    "supertest": "^6.2.4",
    "tap": "^15.1.6"
  },
  "tap": {
    "check-coverage": true,
    "lines": 80
  }
}
